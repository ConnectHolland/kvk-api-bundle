{
    "name": "connectholland/kvk-api-bundle",
    "type": "symfony-bundle",
    "description": "KvK API bundle for Symfony",
    "license": "MIT",
    "require": {
        "csa/guzzle-bundle": "^3.0",
        "jane-php/open-api-runtime": "^5.0",
        "php-http/guzzle6-adapter": "^1.1 || ^2.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0",
        "symfony/translation": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "jane-php/open-api-3": "^5.0"
    },
    "authors": [
        {
            "name": "Matthijs Hasenpflug",
            "email": "matthijs@connectholland.nl"
        }
    ],
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ConnectHolland\\KvkApiBundle\\": ""
        }
    },
    "config": {
         "sort-packages": true
    }
}
